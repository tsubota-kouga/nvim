{
    "clangd.path": "~/.config/coc/extensions/coc-clangd-data/install/14.0.3/clangd_14.0.3/bin/clangd",
    "languageserver": {
        "julia": {
            "command": "/usr/local/bin/julia",
            "args": [
                "/home/kouga/.cache/lsp_neovim/julia/LSPClient/src/LSPClient.jl"
            ],
            "filetypes": [
                "julia"
            ]
        }
    },
    "python.formatting.provider": "black",
    "python.formatting.blackPath": "black",
    "explorer.position": "floating",
    "explorer.width": 30,
    "explorer.icon.enableNerdfont": true,
    "explorer.trash.command": "trash-put %s",
    "explorer.keyMappingMode": "none",
    "explorer.keyMappings.global": {
        "e": "open",
        "i": "open:split",
        "s": "open:vsplit",
        "t": "open:tab",
        "u": [
            "wait",
            "gotoParent"
        ],
        "o": [
            "wait",
            "expanded?",
            "collapse",
            "expand"
        ],
        "p": "pasteFile",
        "I": "toggleHidden",
        "f": "search",
        "F": "searchRecursive",
        "ma": "addFile",
        "md": "delete",
        "mr": "rename",
        "yy": "copyFile",
        "R": "refresh",
        "q": "quit",
        "X": "systemExecute",
        "?": "help",
        "<<": "gitStage",
        ">>": "gitUnstage",
        "<A-h>": "preview:content",
        "<CR>": [
            "expandable?",
            [
                "expanded?",
                "collapse",
                "expand"
            ],
            "open"
        ],
        "<2-LeftMouse>": [
            "expandable?",
            [
                "expanded?",
                "collapse",
                "expand"
            ],
            "open"
        ]
    },
    "suggest.completionItemKindLabels": {
        "keyword": "\uf1de",
        "variable": "\ue79b",
        "value": "\uf89f",
        "operator": "\u03a8",
        "constructor": "\uf0ad",
        "function": "\u0192",
        "reference": "\ufa46",
        "constant": "\uf8fe",
        "method": "\uf09a",
        "struct": "\ufb44",
        "class": "\uf0e8",
        "interface": "\uf417",
        "text": "\ue612",
        "enum": "\uf435",
        "enumMember": "\uf02b",
        "module": "\uf40d",
        "color": "\ue22b",
        "property": "\ue624",
        "field": "\uf9be",
        "unit": "\uf475",
        "event": "\ufacd",
        "file": "\uf723",
        "folder": "\uf114",
        "snippet": "\ue60b",
        "typeParameter": "\uf728",
        "default": "\uf29c"
    },
    "cSpell.userWords": [
        "neovim"
    ]
}
